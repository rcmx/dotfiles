### alias
alias la='ls -a'
alias ll='ls -l'

### git
alias gs='git status'
alias gss='git status -s'
alias gb='git branch '
alias gd='git diff -w'
alias gl="git log --graph --pretty=format:'%Cred%h%Creset -%s %Cgreen(%cr) %C(bold blue)<%an>%Creset'"
alias glol='git log --graph --abbrev-commit --oneline --decorate'

